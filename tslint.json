{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "linterOptions": {
    "exclude": [
      "@types/phaser.d.ts"
    ]
  },
  "jsRules": {},
  "rules": {
    "member-access": [false],
    "no-console": [false],
    "no-var-requires": false,
    "object-literal-key-quotes": [false],
    "object-literal-sort-keys": [false],
    "ordered-imports": [false],
    "quotemark": [
      true,
      "single"
    ],
    "trailing-comma": [false]
  },
  "rulesDirectory": []
}